<script>
  function tab_item(el) {
      console.log("tab_item")
      if(el.classList.contains('active')) return;

      let wrapper = el.closest(".tabs_collection");
      if(wrapper) {
        wrapper.querySelectorAll(".tab_item").forEach(item => item.classList.remove("active"))
        el.classList.add('active')
      }
      let sectionWrapper = el.closest(".collection").querySelectorAll(".slider-mobile-gutter");
        if(sectionWrapper.length > 0) {
          sectionWrapper.forEach( (wrap) => {
          if(el.dataset.id == wrap.dataset.id ) {
            sectionWrapper.forEach( wrap =>  wrap.classList.remove('active') )
            wrap.classList.add('active')
          }
          })
         
        }
    }
</script>
