{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }
  .animation__logos {
    position: absolute;
    width: 200px;
    top: 50px;
    z-index: 5;
    right: 0;
    transform: translateY(-50px);
  }
  .section-{{ section.id }}-padding .slider-button {

    background: white;
    border-radius: 50%;
    position: absolute;
    top: 50%;
    z-index: 3;
    transform: translateY(-50%);
    box-shadow: 0 0 2px 2px #00000036;

  }
  .section-{{ section.id }}-padding .slider-button--prev { 
left: 10px;

  }
   .section-{{ section.id }}-padding .slider-button--next { 
right: 10px;

  }
  
  .section-{{ section.id }}-padding .slider-mobile-gutter {
    display : none;
  }
  .section-{{ section.id }}-padding .slider-mobile-gutter.active {
    display : block;
  }
  .section-{{ section.id }}-padding .swiper {
display : none;
  }
  .tabs_collection {
    margin-top:20px;
        display: flex
;
    align-items: center;
    justify-content: center;
    gap: 1rem;

  }
  .tab_item:not(.active) {
    background: transparent;
    color: black;
  }
    .section-{{ section.id }}-padding .title {
      margin:0;
    }
  .section-{{ section.id }}-padding  .collection__title {
    margin:0;
  }
  .section-{{ section.id }}-padding .product-grid {
    padding : 0;
  }
  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }

  @media screen and (max-width: 749px) {
    .animation__logos {
      display : none !important;
    }
  }
{%- endstyle -%}

{%- liquid
  assign products_to_display = section.settings.collection.all_products_count

  if section.settings.collection.all_products_count > section.settings.products_to_show
    assign products_to_display = section.settings.products_to_show
    assign more_in_collection = true
  endif

  assign columns_mobile_int = section.settings.columns_mobile | plus: 0
  assign show_mobile_slider = false
  if section.settings.swipe_on_mobile and products_to_display > columns_mobile_int
    assign show_mobile_slider = true
  endif

  assign show_desktop_slider = false
  if section.settings.enable_desktop_slider and products_to_display > section.settings.columns_desktop
    assign show_desktop_slider = true
  endif
-%}

<div
  class="color-{{ section.settings.color_scheme }} isolate gradient"
>
  <div
    class="collection{% if section.settings.quick_add == 'bulk' %} collection-quick-add-bulk{% endif %} section-{{ section.id }}-padding{% if section.settings.full_width %} collection--full-width{% endif %}"
    id="collection-{{ section.id }}"
    data-id="{{ section.id }}"
  >
    {% if section.settings.animation_logo %}
        <img src="{{ section.settings.animation_logo | img_url : 'master' }}" class="animation__logos"  />
    {% endif %}
    <div class="collection__title title-wrapper title-wrapper--no-top-margin page-width{% if show_mobile_slider %} title-wrapper--self-padded-tablet-down{% endif %}{% if show_desktop_slider %} collection__title--desktop-slider{% endif %}">
      {%- if section.settings.title != blank -%}
        <h2 class="title inline-richtext {{ section.settings.heading_size }}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
          {{ section.settings.title }}
        </h2>
      {%- endif -%}
      {%- if section.settings.description != blank
        or section.settings.show_description
        and section.settings.collection.description != empty
      -%}
        <div class="collection__description {{ section.settings.description_style }} rte{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
          {%- if section.settings.show_description -%}
            {{ section.settings.collection.description }}
          {%- else -%}
            {{ section.settings.description -}}
          {%- endif %}
        </div>
      {%- endif -%}
    </div>
    {%  if section.blocks.size > 1 %}
    <div class="tabs_collection page-width">
      {% for block in section.blocks %}
      {% if block.settings.collection != blank and block.settings.text != blank %}

          <div class="tab_item {% if forloop.first %} active {% endif %} button" onClick="tab_item(this)" data-id="{{ block.id }}">
            {{ block.settings.text  }}
          </div>

      {% endif %}
      {% endfor %}
    </div>
    {% endif %}
    {% for block in section.blocks %}
      {% if block.settings.collection != blank and block.settings.text != blank %}


    <div class="slider-mobile-gutter {% if forloop.first %} active {% endif %}{% if section.settings.full_width %} slider-component-full-width{% endif %}{% if show_mobile_slider == false %} page-width{% endif %}{% if show_desktop_slider == false and section.settings.full_width == false %} {% endif %}{% if show_desktop_slider %} slider-component-desktop{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}" data-id="{{ block.id }}">
      <div class="swiper">
      <ul
        id="Slider-{{ section.id }}"
        data-id="{{ section.id }}"
        class="swiper-wrapper product-grid contains-card contains-card--product{% if settings.card_style == 'standard' %} contains-card--standard{% endif %} grid--{{ section.settings.columns_desktop }}-col-desktop{% if section.settings.collection == blank %} grid--2-col-tablet-down{% else %} grid--{{ section.settings.columns_mobile }}-col-tablet-down{% endif %}{% if show_mobile_slider or show_desktop_slider %} slider{% if show_desktop_slider %} slider--desktop{% endif %}{% if show_mobile_slider %} slider--tablet grid--peek{% endif %}{% endif %}"
        role="list"
        aria-label="{{ 'general.slider.name' | t }}"
      >
        {% assign skip_card_product_styles = false %}

        {%- if block.settings.collection.products.size > 0 -%}
          {% paginate block.settings.collection.products by section.settings.products_to_show %}
            {%- for product in block.settings.collection.products limit: section.settings.products_to_show -%}
              <li
                id="Slide-{{ section.id }}-{{ forloop.index }}"
                class="swiper-slide {% if show_mobile_slider or show_desktop_slider %} {% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}"
                {% if settings.animations_reveal_on_scroll %}
                  data-cascade
                  style="--animation-order: {{ forloop.index }};"
                {% endif %}
              >
                {% render 'card-product',
                  card_product: product,
                  media_aspect_ratio: section.settings.image_ratio,
                  image_shape: section.settings.image_shape,
                  show_secondary_image: section.settings.show_secondary_image,
                  show_vendor: section.settings.show_vendor,
                  show_rating: section.settings.show_rating,
                  skip_styles: skip_card_product_styles,
                  section_id: section.id,
                  quick_add: section.settings.quick_add
                %}
              </li>
              {%- assign skip_card_product_styles = true -%}
            {%- endfor -%}
          {% endpaginate %}
        {%- else -%}
          {%- for i in (1..section.settings.columns_desktop) -%}
            <li
              class="grid__item{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}"
              {% if settings.animations_reveal_on_scroll %}
                data-cascade
                style="--animation-order: {{ forloop.index }};"
              {% endif %}
            >
              {% liquid
                assign ridx = forloop.rindex
                case ridx
                  when 5
                    assign ridx = 1
                  when 6
                    assign ridx = 2
                endcase
              %}
              {%- assign placeholder_image = 'product-apparel-' | append: ridx -%}
              {% render 'card-product',
                show_vendor: section.settings.show_vendor,
                media_aspect_ratio: section.settings.image_ratio,
                image_shape: section.settings.image_shape,
                placeholder_image: placeholder_image
              %}
            </li>
          {%- endfor -%}
        {%- endif -%}
      </ul>
         <div class="slider-buttons">
          <button
            type="button"
            class="slider-button slider-button--prev"
            name="previous"
            aria-label="{{ 'general.slider.previous_slide' | t }}"
            aria-controls="Slider-{{ section.id }}"
          >
            <span class="svg-wrapper">
              {{- 'icon-caret.svg' | inline_asset_content -}}
            </span>
          </button>
          {% comment %} <div class="slider-counter caption">
            <span class="slider-counter--current">1</span>
            <span aria-hidden="true"> / </span>
            <span class="visually-hidden">{{ 'general.slider.of' | t }}</span>
            <span class="slider-counter--total">{{ products_to_display }}</span>
          </div> {% endcomment %}
          <button
            type="button"
            class="slider-button slider-button--next"
            name="next"
            aria-label="{{ 'general.slider.next_slide' | t }}"
            aria-controls="Slider-{{ section.id }}"
          >
            <span class="svg-wrapper">
              {{- 'icon-caret.svg' | inline_asset_content -}}
            </span>
          </button>
        </div>
        </div>
        

      {%- if show_mobile_slider or show_desktop_slider -%}
        <div class="slider-buttons">
          <button
            type="button"
            class="slider-button slider-button--prev"
            name="previous"
            aria-label="{{ 'general.slider.previous_slide' | t }}"
            aria-controls="Slider-{{ section.id }}"
          >
            <span class="svg-wrapper">
              {{- 'icon-caret.svg' | inline_asset_content -}}
            </span>
          </button>
          {% comment %} <div class="slider-counter caption">
            <span class="slider-counter--current">1</span>
            <span aria-hidden="true"> / </span>
            <span class="visually-hidden">{{ 'general.slider.of' | t }}</span>
            <span class="slider-counter--total">{{ products_to_display }}</span>
          </div> {% endcomment %}
          <button
            type="button"
            class="slider-button slider-button--next"
            name="next"
            aria-label="{{ 'general.slider.next_slide' | t }}"
            aria-controls="Slider-{{ section.id }}"
          >
            <span class="svg-wrapper">
              {{- 'icon-caret.svg' | inline_asset_content -}}
            </span>
          </button>
        </div>
      {%- endif -%}
       {%- if section.settings.show_view_all  -%}
      <div class="center collection__view-all{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
        <a
          href="{{ block.settings.collection.url }}"
          class="{% if section.settings.view_all_style == 'link' %}link underlined-link{% elsif section.settings.view_all_style == 'solid' %}button{% else %}button button--secondary{% endif %}"
          aria-label="{{ 'sections.featured_collection.view_all_label' | t: collection_name: block.settings.collection.title | escape }}"
        >
          {{ 'sections.featured_collection.view_all' | t }}
        </a>
      </div>
    {%- endif -%}
    </div>

   
    {%- endif -%}
  {% endfor %}
    {% if section.settings.image_shape == 'arch' %}
      {{ 'mask-arch.svg' | inline_asset_content }}
    {%- endif -%}
  </div>
</div>


<script>
(() => {
  const STEP_PX = 6;         // px per animation frame (smaller = smoother)
  const PER_SCROLL_PX = 30;  // px to move per scroll event
  const LIMIT_LEFT = -300;   // maximum left translate
  const EL_SELECTOR = '.animation__logos';

  const el = document.querySelector(EL_SELECTOR);
  if (!el) {
    console.warn('Element not found:', EL_SELECTOR);
    return;
  }

  function getBaseTranslateY() {
    const computed = getComputedStyle(el).transform;
    if (!computed || computed === 'none') return 'translateY(-50px)';

    const m = computed.match(/^matrix\((.*)\)$/);
    if (m) {
      const parts = m[1].split(',').map(s => parseFloat(s.trim()));
      if (parts.length === 6 && !Number.isNaN(parts[5])) {
        return `translateY(${parts[5]}px)`;
      }
    }
    return computed;
  }

  const baseTranslateY = getBaseTranslateY();

  let currentX = 0;
  let targetX = 0;
  let animating = false;

  function applyTransform(x) {
    el.style.transform = `${baseTranslateY} translateX(${x}px)`;
  }

  function animateTo(x) {
    if (x < LIMIT_LEFT) x = LIMIT_LEFT;
    if (x > 0) x = 0;

    targetX = x;
    if (!animating) {
      animating = true;
      requestAnimationFrame(step);
    }
  }

  function step() {
    const diff = targetX - currentX;
    if (Math.abs(diff) <= STEP_PX) {
      currentX = targetX;
      applyTransform(currentX);
      animating = false;
      return;
    }
    currentX += diff > 0 ? STEP_PX : -STEP_PX;
    applyTransform(currentX);
    requestAnimationFrame(step);
  }

  el.style.willChange = 'transform';

  let lastScrollY = window.scrollY;
  let lastStepAt = 0;
  const MIN_INTERVAL_MS = 60;

  window.addEventListener('scroll', () => {
    const now = Date.now();
    if (now - lastStepAt < MIN_INTERVAL_MS) {
      lastScrollY = window.scrollY;
      return;
    }
    lastStepAt = now;

    const currentScrollY = window.scrollY;
    const scrollingDown = currentScrollY > lastScrollY;
    lastScrollY = currentScrollY;

    const rect = el.getBoundingClientRect();
    const inView = rect.top < window.innerHeight && rect.bottom > 0;
    if (!inView) return;

    if (scrollingDown) {
      // ✅ SCROLL DOWN → move element LEFT step by step
      const newTarget = Math.max(currentX - PER_SCROLL_PX, LIMIT_LEFT);
      if (newTarget !== currentX) animateTo(newTarget);
    } else {
      // ✅ SCROLL UP → move element RIGHT step by step (towards 0)
      const newTarget = Math.min(currentX + PER_SCROLL_PX, 0);
      if (newTarget !== currentX) animateTo(newTarget);
    }
  }, { passive: true });

})();
</script>


